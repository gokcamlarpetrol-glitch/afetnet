{
  "name": "afetnet",
  "version": "1.0.0",
  "description": "AfetNet - Emergency Communication App",
  "main": "index.ts",
  "packageManager": "npm@10.7.0",
  "engines": {
    "node": ">=18.18.2 <19"
  },
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client --clear --host tunnel",
    "start:lan": "expo start --dev-client --clear --host lan",
    "ios:release": "eas build -p ios --profile production",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "gen:appicons": "node scripts/gen-appicons.js",
    "gen:fullbleed": "node scripts/generate-fullbleed-icons.js",
    "test:iap": "node scripts/test-iap.js",
    "verify:iap": "node scripts/verify-iap-system.js",
    "validate:production": "node scripts/validate-production.js",
    "postinstall": "if [ \"$EAS_BUILD\" != \"true\" ]; then echo 'Local postinstall complete'; else echo 'Skipping postinstall on EAS'; fi",
    "eas-build-pre-install": "echo 'Node:' $(node -v) && echo 'NPM:' $(npm -v)"
  },
  "keywords": [
    "afetnet",
    "emergency",
    "disaster",
    "communication",
    "react-native",
    "expo"
  ],
  "author": "AfetNet Team",
  "license": "ISC",
