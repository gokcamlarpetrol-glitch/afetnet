# AfetNet Server Configuration

# Server Port
PORT=3000

# Environment
NODE_ENV=development

# CORS Settings
CORS_ORIGIN=*

# Rate Limiting (requests per minute)
RATE_LIMIT=100

# Message TTL (milliseconds)
DEFAULT_MESSAGE_TTL=3600000

# Client Timeout (milliseconds)
CLIENT_TIMEOUT=600000

# Log Level
LOG_LEVEL=info
