{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "node": "20.11.1",
      "env": {
        "API_BASE_URL": "https://afetnet-backend.onrender.com",
        "EEW_ENABLED": "false",
        "EEW_NATIVE_ALARM": "false"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "20.11.1",
      "env": {
        "API_BASE_URL": "https://afetnet-backend.onrender.com",
        "EEW_ENABLED": "false",
        "EEW_NATIVE_ALARM": "false"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "simulator": false
      },
      "node": "20.11.1",
      "env": {
        "API_BASE_URL": "https://afetnet-backend.onrender.com",
        "EEW_ENABLED": "false",
        "EEW_NATIVE_ALARM": "false"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {}
    }
  }
}
