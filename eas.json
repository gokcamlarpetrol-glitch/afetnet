{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "simulator": false
      },
      "node": "18.18.2",
      "env": {
        "EXPO_NO_TELEMETRY": "1",
        "NPM_CONFIG_FUND": "false",
        "NPM_CONFIG_AUDIT": "false",
        "NPM_CONFIG_OPTIONAL": "false"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {}
    }
  }
}
