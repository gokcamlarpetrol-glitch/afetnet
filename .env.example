# AfetNet Environment Variables
# Copy this file to .env and fill in your values

# OpenAI API Key (for AI features)
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# RevenueCat API Keys (for in-app purchases)
RC_IOS_KEY=your_revenuecat_ios_key_here
RC_ANDROID_KEY=your_revenuecat_android_key_here

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_PROJECT_ID=your_firebase_project_id_here

# Backend API
API_BASE_URL=https://afetnet-backend.onrender.com
ORG_SECRET=your_org_secret_here

# Feature Flags
EEW_ENABLED=false
EEW_NATIVE_ALARM=false

# App Configuration
APP_NAME=AfetNet
APP_VERSION=1.0.2
BUNDLE_ID=com.gokhancamci.afetnetapp

# EAS Configuration
EAS_PROJECT_ID=072f1217-172a-40ce-af23-3fc0ad3f7f09
